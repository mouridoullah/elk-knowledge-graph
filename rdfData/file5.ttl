@prefix : <http://example.com/catastrophe#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .


<http://example.com/catastrophe>
        rdf:type owl:Ontology ;
        owl:imports <https://raw.githubusercontent.com/mouridoullah/elk-knowledge-graph/main/rdfData/file4.ttl> .


:Catastrophe rdf:type owl:Class ;
    rdfs:subClassOf :Calamité,
                        :Danger,
                        :CauseDeDeces,
                        :Condition ;
    rdfs:label "Catastrophe"@fr ;
    rdfs:comment "Un événement qui cause des dommages importants, des victimes et des perturbations de grande ampleur"@fr .

:CatastropheNaturelle rdf:type owl:Class ;
    rdfs:label "Catastrophe naturelle"@fr ;
    rdfs:subClassOf :Catastrophe .
    # sous-classes de CatastropheNaturelle
    :Inondation rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :Seisme rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :IncendieDeForet rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :Tempete rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :Secheresse rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :GlissementDeTerrain rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :Tsunami rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :Cyclone rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
:CatastropheTechnologique rdf:type owl:Class ;
    rdfs:label "Catastrophe technologique"@fr ;
    rdfs:subClassOf :Catastrophe .
    # sous-classes de CatastropheTechnologique
    :AccidentNucleaire rdf:type owl:Class ; rdfs:subClassOf :CatastropheTechnologique .
    :Explosion rdf:type owl:Class ; rdfs:subClassOf :CatastropheTechnologique .
    :IncendieDeMaison rdf:type owl:Class ; rdfs:subClassOf :CatastropheTechnologique .
    :AccidentDeTrain rdf:type owl:Class ; rdfs:subClassOf :CatastropheTechnologique .

:Calamité rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Calamité"@fr ;
    rdfs:comment "Un événement qui provoque des dégâts matériels et/ou humains"@fr .

:Danger rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Danger"@fr ;
    rdfs:comment "Un événement qui représente un risque pour les personnes, les biens ou l'environnement"@fr .

:CauseDeDeces rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Cause de décès"@fr ;
    rdfs:comment "Un événement qui entraîne la mort d'une ou plusieurs personnes"@fr .

:Condition rdf:type owl:Class ;
    rdfs:subClassOf owl:Thing ;
    rdfs:label "Condition"@fr ;
    rdfs:comment "Un état qui peut causer des dommages ou des perturbations"@fr .

:aPourDate rdf:type owl:ObjectProperty ;
    rdfs:domain :Catastrophe, :Sauvetage ;
    rdfs:range xsd:date ;
    rdfs:label "La date"@fr ;
    rdfs:comment "La date de l'événement"@fr .

:aLieu rdf:type owl:ObjectProperty ;
    rdfs:domain :Catastrophe, :Sauvetage;
    rdfs:range <http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing> ;
    rdfs:label "Le lieu"@fr ;
    rdfs:comment "Le lieu de l'événement"@fr .

:Victimes rdf:type owl:Class ;
    rdfs:label "Victimes" ;
    rdfs:comment "Les victimes sont des personnes touchées par une catastrophe, qui peuvent être blessées, mortes ou disparues." ;
    rdfs:subClassOf owl:Thing .

:NombreDeVictimes rdf:type owl:ObjectProperty ;
  rdfs:label "Nombre de victimes" ;
  rdfs:comment "Nombre de victimes affectée par la catastrophe." ;
  rdfs:domain :Victimes ;
  rdfs:range xsd:int .

:Implique rdf:type owl:ObjectProperty ;
    rdfs:domain :Catastrophe ;
    rdfs:range :Victimes ;
    rdfs:label "Nombre de survivants"@fr ;
    rdfs:comment "Le nombre de personnes qui ont survécu à la catastrophe"@fr .

:Morts rdf:type owl:Class ;
    rdfs:label "Morts" ;
    rdfs:comment "Les morts sont des victimes qui ont perdu la vie dans une catastrophe." ;
    rdfs:subClassOf :Victimes .

:Blesses rdf:type owl:Class ;
    rdfs:label "Blessés" ;
    rdfs:comment "Les blessés sont des victimes qui ont été blessées lors d'une catastrophe." ;
    rdfs:subClassOf :Victimes .

:Disparus rdf:type owl:Class ;
    rdfs:label "Disparus" ;
    rdfs:comment "Les disparus sont des victimes dont le sort est inconnu après une catastrophe." ;
    rdfs:subClassOf :Victimes .

# Définition de la classe Sauvetage
:Sauvetage rdf:type owl:Class ;
  rdfs:label "Sauvetage" ;
  rdfs:comment "Les opérations de sauvetage après une catastrophe." .

:Soigner rdf:type owl:Class ;
  rdfs:subClassOf :Sauvetage ;
  rdfs:label "Soigner" ;
  rdfs:comment "L'opération de soins pour les blessés." .

:OperationDeSauvetage rdf:type owl:ObjectProperty ;
    rdfs:domain :Sauvetage ;
    rdfs:range :Victimes ;
    rdfs:label "Opération Sauvetage" .

:OperationDeSoins rdf:type owl:ObjectProperty ;
    rdfs:domain :Blesses ;
    rdfs:range :Soigner ;
    rdfs:label "Opération de soins associée" ;
    rdfs:comment "L'opération de soins pour les blessés." ;
    rdfs:subPropertyOf :OperationDeSauvetage .

:Recherche rdf:type owl:Class ;
  rdfs:subClassOf :Sauvetage ;
  rdfs:label "Recherche" ;
  rdfs:comment "L'opération de recherche pour les disparus." .

:OperationDeRecherche rdf:type owl:ObjectProperty ;
    rdfs:domain :Disparus ;
    rdfs:range :Recherche ;
    rdfs:label "Opération de recherche associée" ;
    rdfs:comment "L'opération de recherche associée à un groupe de disparus." ;
    rdfs:subPropertyOf :OperationDeSauvetage .

:RecupererIdentifierTraiter rdf:type owl:Class ;
  rdfs:subClassOf :Sauvetage ;
  rdfs:label "Recuperer, identifier et traiter les morts" ;
  rdfs:comment "L'opération de récupération pour les morts." .

:OperationTraitementMorts rdf:type owl:ObjectProperty ;
    rdfs:domain :Morts ;
    rdfs:range :RecupererIdentifierTraiter;
    rdfs:label "Opération de Traitement des morts" ;
    rdfs:comment "L'opération de secourisme." ;
    rdfs:subPropertyOf :OperationDeSauvetage .

:NombreDeSecouriste rdf:type owl:ObjectProperty ;
  rdfs:label "Effectif des secouriste" ;
  rdfs:comment "Le nombre de secouristes mobilisés." ;
  rdfs:domain :EquipeDeSecours ;
  rdfs:range xsd:int .

:Effectue rdf:type owl:ObjectProperty ;
          rdfs:domain :EquipeDeSecours ;
          rdfs:range :Sauvetage .

# Définition des classes Pompier, Médecin, Militaire et Infirmier
:Pompiers rdf:type owl:Class ;
  rdfs:label "Pompier" ;
  rdfs:comment "Personnel de secours spécialisé dans l'extinction d'incendies." .

:Médecin rdf:type owl:Class ;
  rdfs:label "Médecin" ;
  rdfs:comment "Personnel de secours spécialisé dans les soins médicaux." .

:Militaire rdf:type owl:Class ;
  rdfs:label "Militaire" ;
  rdfs:comment "Personnel de secours spécialisé dans les opérations militaires." .

:Infirmier rdf:type owl:Class ;
  rdfs:label "Infirmier" ;
  rdfs:comment "Personnel de secours spécialisé dans les soins infirmiers." .

# Définition de la propriété NombreDeSecouriste pour chaque type de personnel
:NombreDindividus rdf:type owl:ObjectProperty ;
  rdfs:label "Effectif des secouriste" ;
  rdfs:comment "Le nombre de pompiers mobilisés." ;
  rdfs:domain :Pompiers, :Militaire, :Médecin, :Infirmier;
  rdfs:range xsd:int ;
  rdfs:subPropertyOf :NombreDeSecouriste .

:EstConsitutéDe rdf:type owl:ObjectProperty ;
    rdfs:domain :Pompiers, :Médecin, :Militaire, :Infirmier ;
    rdfs:range :EquipeDeSecours.


# Définition de la classe Dommage
:Dommages rdf:type owl:Class ;
    rdfs:label "Dommage"@fr ;
    rdfs:subClassOf owl:Thing;
    rdfs:comment "Classe représentant un dommage matériel causé à un objet ou une propriété"@fr .

# sous-classes de Dommages
:PerteEconomique rdf:type owl:Class ; rdfs:subClassOf :Dommages .
:DomagesEnvironnementaux rdf:type owl:Class ; rdfs:subClassOf :Dommages .
:DommagesMateriels rdf:type owl:Class ;
    rdfs:label "Dommages matériels"@fr ;
    rdfs:subClassOf :Dommages .
:DestructionDeBatiments rdf:type owl:Class ; rdfs:subClassOf :DommagesMateriels .
:DestructionInfrastructure rdf:type owl:Class ; rdfs:subClassOf :DommagesMateriels .

:coutDommage rdf:type owl:ObjectProperty ;
    rdfs:label "Coût du dommage"@fr ;
    rdfs:domain :Dommages ;
    rdfs:range xsd:int ;
    rdfs:comment "Le coût associé au dommage causé par l'événement"@fr .

:Entraine rdf:type owl:ObjectProperty ;
    rdfs:domain :Catastrophe ;
    rdfs:range :Dommages ;
    rdfs:label "Consequence d'une catastrophe"@fr ;
    rdfs:comment "Dégâts matériels causés par une catastrophe"@fr .

:Retablissement rdf:type owl:Class ; 
    rdfs:subClassOf owl:Thing; 
    rdfs:label "Retablissement"@fr;
    rdfs:comment "Retablissement et réparation des dommages"@fr.

# Extension des définitions des rétablissements
:Reconstruction rdf:type owl:Class ;
    rdfs:label "Reconstruction"@fr ;
    rdfs:subClassOf :Retablissement .
:ReconstructionDeBatiments rdf:type owl:Class ;
    rdfs:label "Reconstruction de bâtiments"@fr ;
    rdfs:subClassOf :Reconstruction ;
    rdfs:comment "Type de rétablissement pour les dommages de type destruction de bâtiments"@fr .
:ReconstructionInfrastructure rdf:type owl:Class ;
    rdfs:label "Reconstruction d'infrastructures"@fr ;
    rdfs:subClassOf :Reconstruction ;
    rdfs:comment "Type de rétablissement pour les dommages de type destruction d'infrastructures"@fr .
:RestaurerLesEcosystèmes rdf:type owl:Class ;
    rdfs:label "Restaurer Les Ecosystèmes"@fr ;
    rdfs:subClassOf :Retablissement .
:Relogement rdf:type owl:Class ;
    rdfs:label "Relogement"@fr ;
    rdfs:subClassOf :Retablissement .
:EvaluerIndemniserLesVictimes rdf:type owl:Class ;
    rdfs:label "Relogement"@fr ;
    rdfs:subClassOf :Retablissement .

:aPourRetablissement rdf:type owl:ObjectProperty ;
    rdfs:label "a pour rétablissement"@fr ;
    rdfs:domain :Dommages ;
    rdfs:range :Retablissement ;
    rdfs:comment "Propriété qui lie un dommage à son type de rétablissement"@fr .
