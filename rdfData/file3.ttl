@prefix : <http://example.com/catastrophe#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

<http://example.com/catastrophe>
        rdf:type owl:Ontology .

:Catastrophe rdf:type owl:Class ;
        rdfs:subClassOf :Calamité,
                         :Danger,
                         :CauseDeDeces,
                         :Condition ;
        owl:hasKey :numero ;

        :nombreDeMorts rdf:type owl:DatatypeProperty ;
                       rdfs:range xsd:nonNegativeInteger .

        :nombreDeVictimes rdf:type owl:DatatypeProperty ;
                          rdfs:range xsd:nonNegativeInteger .

        :nombreDeSurvivants rdf:type owl:DatatypeProperty ;
                            rdfs:range xsd:nonNegativeInteger .

        :nombreDeDisparus rdf:type owl:DatatypeProperty ;
                          rdfs:range xsd:nonNegativeInteger .

        :nombreDeBlesses rdf:type owl:DatatypeProperty ;
                          rdfs:range xsd:nonNegativeInteger .

        :coutDesDommages rdf:type owl:DatatypeProperty ;
                          rdfs:range xsd:nonNegativeInteger .

        :coutImmediat rdf:type owl:DatatypeProperty ;
                      rdfs:range xsd:nonNegativeInteger .

        :aDetruit rdf:type owl:ObjectProperty ;
                  rdfs:domain :Catastrophe ;
                  rdfs:range :Condition .

        :implique rdf:type owl:ObjectProperty ;
                  rdfs:domain :Catastrophe ;
                  rdfs:range :Condition .

:Calamité rdf:type owl:Class .

:Danger rdf:type owl:Class .

:CauseDeDeces rdf:type owl:Class .

:Condition rdf:type owl:Class .

