@prefix : <http://example.org/ontology#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

# Import de l'ontologie
@prefix ont: <https://raw.githubusercontent.com/mouridoullah/elk-knowledge-graph/main/rdfData/file5.ttl#> .

# Définition de l'instance de Pompiers avec la propriété NombreDePompiers
:InstanceDePompiers rdf:type ont:Pompiers ;
          rdfs:label "Pompier" ;
          ont:NombreDindividus "100"^^xsd:int .

:InstanceDeMédecins rdf:type ont:Médecin ;
          rdfs:label "Médecin" ;
          ont:NombreDindividus "100"^^xsd:int .

:InstanceDeMilitaires rdf:type ont:Militaire ;
          rdfs:label "Militaire" ;
          ont:NombreDindividus "100"^^xsd:int .

:InstanceDeInfirmiers rdf:type ont:Infirmier ;
          rdfs:label "Infirmier" ;
          ont:NombreDindividus "100"^^xsd:int .
