@prefix : <http://www.semanticweb.org/ontologies/2023/4/seisme#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:MyOntology a owl:Ontology ;
    owl:imports <https://raw.githubusercontent.com/mouridoullah/elk-knowledge-graph/main/rdfData/file.ttl> .



# Définition des concepts
:Catastrophe rdf:type owl:Class ;
    rdfs:label "Catastrophe"@fr ;
    rdfs:subClassOf owl:Thing .
    # sous-classes de Catastrophe
    :CatastropheNaturelle rdf:type owl:Class ;
        rdfs:label "Catastrophe naturelle"@fr ;
        rdfs:subClassOf :Catastrophe .
        # sous-classes de CatastropheNaturelle
        :Inondation rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
        :Seisme rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
        :IncendieDeForet rdf:type owl:Class ; rdfs:subClassOf :CatastropheNaturelle .
    :CatastropheTechnologique rdf:type owl:Class ;
        rdfs:label "Catastrophe technologique"@fr ;
        rdfs:subClassOf :Catastrophe .
        # sous-classes de CatastropheTechnologique
        :AccidentNucleaire rdf:type owl:Class ; rdfs:subClassOf :CatastropheTechnologique .
        :Explosion rdf:type owl:Class ; rdfs:subClassOf :CatastropheTechnologique .

:Victime rdf:type owl:Class ;
    rdfs:label "Victime"@fr ;
    rdfs:subClassOf owl:Thing .
    # sous-classes de Victime
    :Mort rdf:type owl:Class ; rdfs:subClassOf :Victime .
    :Blesse rdf:type owl:Class ; rdfs:subClassOf :Victime .
    :Disparu rdf:type owl:Class ; rdfs:subClassOf :Victime .

:Sauvetage rdf:type owl:Class ;
    rdfs:label "Opération de sauvetage"@fr ;
    rdfs:subClassOf owl:Thing .
    # sous-classes de Sauvetage
    :EquipeDeSecours rdf:type owl:Class ; rdfs:subClassOf :Sauvetage .
    :CentreDhebergement rdf:type owl:Class ; rdfs:subClassOf :Sauvetage .
    :CentreMedical rdf:type owl:Class ; rdfs:subClassOf :Sauvetage .
    :Deplacement rdf:type owl:Class ; rdfs:subClassOf :Sauvetage .

:Dommages rdf:type owl:Class ;
    rdfs:label "Dommages"@fr ;
    rdfs:subClassOf owl:Thing .
    # sous-classes de Dommages
    :PerteEconomique rdf:type owl:Class ; rdfs:subClassOf :Dommages .
    :DestructionDeBatiments rdf:type owl:Class ; rdfs:subClassOf :Dommages .
    :DomagesEnvironnementaux rdf:type owl:Class ; rdfs:subClassOf :Dommages .

:Preparation rdf:type owl:Class ;
    rdfs:label "Préparation"@fr ;
    rdfs:subClassOf owl:Thing .
    # sous-classes de Preparation
    :PlanDevacuation rdf:type owl:Class ; rdfs:subClassOf :Preparation .
    :Formation rdf:type owl:Class ; rdfs:subClassOf :Preparation .
    :EquipementDePrevention rdf:type owl:Class ; rdfs:subClassOf :Preparation .
    :Evaluation rdf:type owl:Class ; rdfs:subClassOf :Preparation .
    :Retablissement rdf:type owl:Class ; rdfs:subClassOf :Preparation .
