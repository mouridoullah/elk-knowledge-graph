@prefix : <http://www.semanticweb.org/ontologies/2023/4/city#> .
@prefix loria: <http://exemple.org/ontology/loria/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix dbo: <http://dbpedia.org/ontology/> .


:MyOntology a owl:Ontology ;
  owl:imports <https://raw.githubusercontent.com/mouridoullah/elk-knowledge-graph/main/rdfData/cata.ttl> .

loria:write rdf:type owl:ObjectProperty ;
  rdfs:domain foaf:Person ;
  rdfs:range foaf:Document ;
  owl:inverseOf loria:writtenBy ;
  rdfs:subPropertyOf dcterms:creator .


loria:nearTo rdf:type owl:ObjectProperty , owl:SymmetricProperty ;
  rdfs:domain dbo:PopulatedPlace ;
  rdfs:range dbo:PopulatedPlace .
