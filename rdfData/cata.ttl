@prefix : <http://www.semanticweb.org/ontologies/2023/4/city#> .
@prefix loria: <http://exemple.org/ontology/loria/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/> .
@prefix dbo: <http://dbpedia.org/ontology/> .
@prefix city: <http://example.org/city/> .


:MyOntology a owl:Ontology ;
  owl:imports <https://raw.githubusercontent.com/mouridoullah/elk-knowledge-graph/main/rdfData/cata.ttl> .

loria:write rdfs:domain foaf:Person .
loria:write rdfs:range foaf:Document .
loria:write owl:inverseOf loria:writtenBy .
loria:write rdfs:subPropertyOf dcterms:creator .

ex:Fantacy skos:broaderTransitive ex:Story .

loria:nearTo a owl:SymmetricProperty .
loria:nearTo rdfs:domain dbo:PopulatedPlace .
loria:nearTo rdfs:range dbo:PopulatedPlace .
city:Paris loria:nearTo city:Versailles .
